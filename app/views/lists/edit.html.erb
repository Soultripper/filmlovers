<%= render layout: 'layouts/user', object: user do%>
  <div id="userList" class="twelve columns">
    <div class="twelve columns">
      <h6 class="heading_divider">Edit list</h6>
    </div>           
    <%= form_for list, url: list_path(list), method: :put do |l|%>
      <%= render partial: 'form', locals:{list_form: l} %>
    <% end %>
  </div>
<% end %>