<%= render layout: 'layouts/user', object: user do%>
  <div id="userList" class="twelve columns">
    <div class="twelve columns">
      <h6 class="heading_divider">New list</h6>
    </div>         
    <%= form_for list, url: lists_path, method: :post do |l|%>
      <%= render partial: 'form', locals:{list_form: l} %>
    <% end %>
  </div>
<% end %>