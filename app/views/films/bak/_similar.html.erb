<% set_meta_tags :title => "#{title_with_year(film)} - similar films" %>

<div class="large-12 columns" id="film-content-similar">

  <div class="large-9 columns films" id="filmsContent" >
  <% if film.similar? %>
    <%= render partial: 'films/film', collection: film.similar, as: :film %>
  <% end %>
 
</div>